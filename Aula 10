# Tuplas

tupla = (1, 2)
print (tupla)

tupla += (3, 4)
print (tupla)

tupla = tupla * 2
print (tupla)

for n in tupla:
    print(n)

t = tuple(range(1,11))
print(t)

lista = []
for n in range(1,11):
    lista.append(n)
    print(tuple(lista))

# a = tuple(list(input()))
# print(a)

